<h1>{{ 'Voucher'|trans }}</h1>
<hr />
<!-- Breadcrumbs-->
<ol class="breadcrumb">
    <li class="breadcrumb-item">{{ 'Shop'|trans }}</li>
    <li class="breadcrumb-item active">{{ 'Voucher'|trans }}</li>
</ol>
<hr />
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>{{ 'Code'|trans }}</th>
            <th>{{ 'Type'|trans }}</th>
            <th>{{ 'Amount'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for voucher in vouchers %}
            <tr>
                <td>{{ voucher.code }}</td>
                <td>{{ voucher.type }}</td>
                <td>{{ voucher.amount }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
