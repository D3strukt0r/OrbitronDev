<section id="header">
    <div class="col-lg-12">
        <h1 class="page-header">{{ 'Voucher'|trans }}</h1>
        <ol class="breadcrumb">
            <li>{{ 'Shop'|trans }}</li>
            <li class="active">{{ 'Voucher'|trans }}</li>
        </ol><!-- /.breadcrumb -->
        <hr />
    </div><!-- /.col-lg-12 -->
</section><!-- /#header -->
<section id="page">
    <div class="col-lg-12">
        <table class="table table-hover table-striped">
            <thead>
                <tr>
                    <th>{{ 'Code'|trans }}</th>
                    <th>{{ 'Type'|trans }}</th>
                    <th>{{ 'Amount'|trans }}</th>
                </tr>
            </thead>
            <tbody>
                {% for voucher in vouchers %}
                    <tr>
                        <td>{{ voucher.code }}</td>
                        <td>{{ voucher.type }}</td>
                        <td>{{ voucher.amount }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
        <!-- /.table .table-hover -->
    </div>
</section><!-- /#page -->
